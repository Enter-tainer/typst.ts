{
  "name": "@myriaddreamin/typst.ts",
  "version": "0.1.6",
  "description": "run Typst in JavaScriptWorld.",
  "keywords": [
    "TypeScript",
    "Typst"
  ],
  "author": "Myriad-Dreamin",
  "license": "Apache-2.0",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build:wasm": "cd ../renderer/ && yarn run build",
    "build:wasm:dev": "cd ../renderer/ && yarn run build:dev",
    "build:dev": "yarn run build:wasm:dev && tsc --emitDeclarationOnly --outDir dist && node esbuild.config.mjs",
    "build": "yarn run build:wasm && tsc --emitDeclarationOnly --outDir dist && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "link:local": "yarn link @myriaddreamin/typst-ts-renderer && yarn link",
    "prepublish": "npm run build",
    "publish:dry": "npm publish --dry-run --access public",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/web": "^0.0.99",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "esbuild-plugin-wasm": "^1.0.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
